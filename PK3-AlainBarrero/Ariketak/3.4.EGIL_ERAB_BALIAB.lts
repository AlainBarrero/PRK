range Bool = 0..5

EGIL = (egin -> EGIL).
ERAB = (eskuratu->erabili->askatu->ERAB).

BALIAB = BALIAB[0],
BALIAB[i:Bool] = ( when (i==0) x.egin -> BALIAB[3]
				 | when (i==1) x.egin -> BALIAB[4]
				 | when (i==0) y.egin -> BALIAB[1]
				 | when (i==3) y.egin -> BALIAB[4]



				 | when (i==4) {a,b}.eskuratu 	-> BALIAB[5]
				 | when (i==5) {a,b}.askatu 	-> BALIAB[0] ).

||ERAB_BALIAB = ({a,b}:ERAB|| {x,y}:EGIL ||BALIAB).
